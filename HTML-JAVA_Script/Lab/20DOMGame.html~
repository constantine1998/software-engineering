<html>

<head>
 <script type="text/javascript">
a=16
x=16
	function up () {
		upper = a-4;
		if (upper<0) {return;}
		 element1 = document.getElementById(x.toString())
		 element2 = document.getElementById(upper.toString())
    		clonedElement1 = element1.cloneNode(true);
    		clonedElement2 = element2.cloneNode(true);

    		element2.parentNode.replaceChild(clonedElement1, element2);
    		element1.parentNode.replaceChild(clonedElement2, element1);
a=a-4;
alert(a)

}

function down () {
                downer = a;
		if (downer>17) {return;}
		element1 = document.getElementById(x.toString())
		element2 = document.getElementById(downer.toString())
    		clonedElement1 = element1.cloneNode(true);
    		clonedElement2 = element2.cloneNode(true);
    		element2.parentNode.replaceChild(clonedElement1, element2);
    		element1.parentNode.replaceChild(clonedElement2, element1);
a=a+4;
alert(a);
}


function left () {
                lefter = a-1;
		if (lefter<a-4) {return;}
		element1 = document.getElementById(x.toString())
		element2 = document.getElementById(lefter.toString())
    		clonedElement1 = element1.cloneNode(true);
    		clonedElement2 = element2.cloneNode(true);
    		element2.parentNode.replaceChild(clonedElement1, element2);
    		element1.parentNode.replaceChild(clonedElement2, element1);
a=a-1;
alert(a)
}

function right () {
                righter = a-1;
		if (righter>a-4) {return;}
		element1 = document.getElementById(x.toString())
		element2 = document.getElementById(righter.toString())
    		clonedElement1 = element1.cloneNode(true);
    		clonedElement2 = element2.cloneNode(true);
    		element2.parentNode.replaceChild(clonedElement1, element2);
    		element1.parentNode.replaceChild(clonedElement2, element1);
a=a-1;
}

 </script>
</head>

<body>


